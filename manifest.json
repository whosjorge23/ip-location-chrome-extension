{
  "manifest_version": 2,
  "name": "IP Location",
  "version": "1.0.1",
  "author": {
    "name": "Giorgio Giannotta",
    "email": "jorge_upa@msn.com",
    "url": "http://www.westcostyle.com"
  },
  "description": "Displays your IP address and location",
  "icons": {
    "16": "/icons/ip16.png",
    "48": "/icons/ip48.png",
    "128": "/icons/ip128.png"
  },
  "permissions": ["https://api.ipify.org/", "https://ipapi.co/"],
  "browser_action": {
    "default_title": "IP Location",
    "default_icon": {
      "16": "/icons/ip16.png",
      "32": "/icons/ip48.png",
      "48": "/icons/ip48.png",
      "128": "/icons/ip128.png"
    },
    "default_popup": "popup.html"
  }
}
